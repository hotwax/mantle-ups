    <mantle.shipment.ShipmentBoxType shipmentBoxTypeId="UPS_Tube" description="UPS Tube"
                                     dimensionUomId="LEN_in" boxLength="38.00" boxWidth="6.00" boxHeight="6.00" gatewayBoxId="03"/>
    <mantle.shipment.ShipmentBoxType shipmentBoxTypeId="UPS_Package" description="UPS Package"
                                     dimensionUomId="LEN_in" boxLength="15.00" boxWidth="15.00" boxHeight="6.00" gatewayBoxId="02"/>

    <mantle.shipment.carrier.ShippingGatewayConfig shippingGatewayConfigId="UPS_DEMO"
                                                   shippingGatewayTypeEnumId="ShGtwyUPS" description="UPS API Demo"
                                                   getRateServiceName="mantle.UpsServices.get#UpsShippingRate"
                                                   requestLabelsServiceName="mantle.UpsServices.delete#CreateLabel">

        <methods carrierPartyId="UPS" shipmentMethodEnumId="ShMthNextDay" gatewayServiceCode="01"/>
        <methods carrierPartyId="UPS" shipmentMethodEnumId="ShMthGround" gatewayServiceCode="03"/>

        <!--  set your ShipEngine API token (test or live) with a record like this  -->
        <options optionEnumId="SgoUPSUsername" optionValue=""/>
        <options optionEnumId="SgoUPSPassword" optionValue=""/>
        <options optionEnumId="SgoUPSShipperNumber" optionValue=""/>
        <options optionEnumId="SgoLabelType" optionValue="PDF_4x6"/>

    </mantle.shipment.carrier.ShippingGatewayConfig>

    <!--  used in mantle.shipment.CarrierServices ShipmentLabels services  -->
    <moqui.security.UserGroupPreference userGroupId="ALL_USERS" preferenceKey="DefaultShipmentGatewayConfigId" preferenceValue="UPS_DEMO"/>

    <!--  example per org/vendor settings, used as defaults if not store; note that for screens like EditParty with no store or
            vendor the Owner Party must be set to use these settings  -->
    <mantle.party.PartySetting partyId="ORG_ZIZI_RETAIL" partySettingTypeId="DefaultShipmentGatewayConfigId" settingValue="UPS_DEMO"/>

    <mantle.product.store.ProductStore productStoreId="POPC_DEFAULT">
        <shipOptions carrierPartyId="UPS" shipmentMethodEnumId="ShMthNextDay" />
        <shipOptions carrierPartyId="UPS" shipmentMethodEnumId="ShMthGround" />
    </mantle.product.store.ProductStore>

</entity-facade-xml>